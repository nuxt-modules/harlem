{
  "name": "@nuxtjs/harlem",
  "version": "1.1.0",
  "license": "MIT",
  "description": "Harlem integration for Nuxt",
  "repository": "nuxt-modules/harlem",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "harlem",
    "store"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "prepack": "pnpm build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "test:e2e": "vitest run test/e2e"
  },
  "dependencies": {
    "@harlem/core": "^2.3.9",
    "@harlem/plugin-devtools": "^2.3.9",
    "@harlem/plugin-ssr": "^2.3.9",
    "@nuxt/kit": "^3.0.0-rc.9",
    "knitwork": "^0.1.2",
    "scule": "^0.3.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.1.7",
    "@nuxt/schema": "3.0.0-rc.9",
    "@nuxt/test-utils": "npm:@nuxt/test-utils-edge@latest",
    "@nuxtjs/eslint-config-typescript": "11.0.0",
    "@release-it/conventional-changelog": "5.1.0",
    "@types/serve-static": "1.15.0",
    "@vitest/coverage-c8": "^0.22.1",
    "c8": "^7.12.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "nuxt": "npm:nuxt3@latest",
    "playwright": "^1.25.1",
    "prettier": "2.7.1",
    "release-it": "15.4.1",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "resolutions": {
    "@nuxtjs/harlem": "link:./",
    "@nuxt/vite-builder": "npm:@nuxt/vite-builder-edge@latest"
  },
  "packageManager": "pnpm@7.9.3"
}
